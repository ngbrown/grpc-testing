{
  "name": "grpc-greeter-client",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:protoc && tsc",
    "build:protoc": "protoc --ts_out ./src/gen --ts_opt eslint_disable --ts_opt ts_nocheck --ts_opt long_type_string --proto_path ./Protos ./Protos/greet.proto && node ./tools/patch-esm.mjs",
    "dev": "vite-node ./src/index.ts",
    "start": "node ./dist/index.js"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "@tsconfig/node18": "^18.2.4",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "vite-node": "^2.0.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.11",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "@protobuf-ts/runtime": "^2.9.4"
  }
}